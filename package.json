	"license": "MIT",
	"dependencies": {
		"axios": "^0.18.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.1",
		"concurrently": "^4.0.1",
		"express": "^4.18.2",
		"gravatar": "^1.6.0",
		"jsonwebtoken": "^8.3.0",
		"mongoose": "^5.9.7",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"validator": "^10.8.0"
	},
	"devDependencies": {
		"nodemon": "^1.19.4"
	}
}
